<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetAndroidSS"
        tools:targetApi="31" >
        <activity
            android:name=".SessionFormationActivity"
            android:exported="false"
            android:label="@string/title_activity_session_formation"
            android:theme="@style/Theme.ProjetAndroidSS" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SessionActivity"
            android:exported="false"
            android:label="@string/title_activity_session"
            android:theme="@style/Theme.ProjetAndroidSS" />
        <activity
            android:name=".FirstActivity"
            android:exported="true"
            android:label="@string/title_activity_first"
            android:theme="@style/Theme.ProjetAndroidSS" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:exported="false"
            android:label="HomeActivity"
            android:theme="@style/Theme.ProjetAndroidSS" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:label="ProfileActivity"
            android:theme="@style/Theme.ProjetAndroidSS" />
        <activity
            android:name=".StudentInfoActivity"
            android:exported="false"
            android:label="StudentInfoActivity"
            android:theme="@style/Theme.ProjetAndroidSS" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
    </application>

</manifest>